    }\n  }\n\n  .section {\n    h1 {\n      margin-bottom: 50px;\n      color: $orange;\n      text-transform: uppercase;\n      font-weight: 300;\n      font-size: 36px;\n    }\n\n    p {\n      line-height: 1.8em;\n      margin-bottom: 30px;\n    }\n\n    .ui.cards {\n      .card {\n        border-radius: 0;\n        box-shadow: 0 1px 3px 0 #D4D4D5,0 0 0 1px #f1f1f2;\n      }\n    }\n\n    ul {\n      margin: 0;\n      padding: 0;\n\n      &.no-margin {\n        li {\n          &:first-child {\n            margin-top: 0;\n          }\n\n          &:last-child {\n            margin-bottom: 0;\n          }\n        }\n      }\n\n      li {\n        list-style: none;\n        padding-left: 30px;\n        position: relative;\n        margin: 15px 0;\n\n        &:before {\n          position: absolute;\n          font-family: Icons;\n          content: \"\\f046\";\n          color: $orange;\n          left: 0;\n          font-size: 15px;\n          top: 2px;\n        }\n      }\n    }\n\n    &.orange-background, &.teal-background {\n      h1 {\n        color: #FFF;\n      }\n\n      ul li:before {\n        color: #FFF;\n      }\n\n      p {\n        color: #FFF;\n      }\n\n      p a {\n        color: #FFF;\n        font-weight: 600;\n      }\n    }\n\n    &.logos {\n      img {\n        max-width: 100%;\n        margin: 20px 0;\n        display: block;\n      }\n    }\n\n    &.become {\n      li:before {\n        content: '\\f10c';\n      }\n\n      ul + p {\n        margin: 25px 0;\n      }\n    }\n  }\n\n}\n",".section {\n  padding: 70px 0 !important;\n\n  &.no-padding-top {\n    padding-top: 0 !important;\n  }\n\n  &.no-padding-bottom {\n    padding-bottom: 0 !important;\n  }\n}\n\n.hidden {\n  display: none;\n}\n\n.visuallyhidden {\n  border: 0;\n  clip: rect(0 0 0 0);\n  height: 1px;\n  margin: -1px;\n  overflow: hidden;\n  padding: 0;\n  position: absolute;\n  width: 1px;\n}\n\n.ui.button {\n  border-radius: 0 !important;\n  opacity: 1;\n  transition: 0.3s ease 