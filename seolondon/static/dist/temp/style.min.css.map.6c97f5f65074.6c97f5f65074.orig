slider {\n  position: relative;\n\n  .unslider-arrow {\n    color: rgba(0, 0, 0, 0);\n    top: 47%;\n    font-size: 0;\n\n    &:hover {\n      &:before {\n        opacity: 0.7;\n      }\n    }\n\n    &.next {\n      //right: 40px;\n\n      &:before {\n        content: \"\\f138\";\n        font-family: Icons;\n        font-size: 40px;\n        color: white;\n      }\n    }\n\n    &.prev {\n      //left: 40px;\n\n      &:before {\n        content: \"\\f137\";\n        font-family: Icons;\n        font-size: 40px;\n        color: white;\n      }\n    }\n  }\n\n  .unslider-nav {\n    margin-bottom: 30px;\n\n    li {\n      width: 12px;\n      height: 12px;\n      border-radius: 999px;\n    }\n  }\n}\n\n.masthead {\n  background-size: cover;\n  background-repeat: no-repeat;\n  background-position: center center;\n  width: 100%;\n  position: relative;\n\n  .button {\n    margin-top: 25px;\n  }\n\n  &:not(.about-page) {\n\n    &:before {\n      content: '';\n      display: block;\n      position: absolute;\n      top: 0;\n      right: 0;\n      bottom: 0;\n      left: 0;\n      background: rgba(0, 0, 0, 0.2);\n    }\n\n    >.grid {\n      height: 75vh;\n    }\n\n  }\n\n  $span-opacity: 0.6;\n\n  .grid h1.blue span {\n    background-color: rgba($blue, $span-opacity);\n  }\n\n  &.connect {\n    .grid h1 span {\n      background-color: rgba($red, $span-opacity);\n    }\n  }\n\n  &.scholars {\n    .grid h1 span {\n      background-color: rgba($orange, $span-opacity);\n    }\n  }\n\n  &.careers-page {\n    .grid h1 span {\n      background-color: rgba($teal, $span-opacity);\n    }\n    .buttons {\n      text-align: center !important;\n      .button {\n        display: block;\n        @include mobile {\n          margin: 10px 0;\n        }\n      }\n    }\n  }\n\n  .buttons {\n    .button {\n      display: inline-block;\n      max-width: 175px;\n      margin-left: auto;\n      margin-bottom: 5px;\n\n      @include not-computer {\n        margin: 0;\n      }\n    }\n  }\n\n  .grid {\n\n    h1 {\n      font-family: \"Lato\", sans-serif;\n      font-weight: 300 !important;\n      font-size: 2rem;\n      text-transform: uppercase;\n      line-height: 3rem;\n\n      span {\n        color: #FFF;\n        padding: 5px 15px;\n        background-color: rgba($teal, $span-opacity);\n\n        @include not-computer {\n          display: block;\n        }\n      }\n    }\n\n    h2 {\n      span {\n        color: #FFF;\n        padding: 5px 15px;\n        background-color: rgba($teal, $span-opacity);\n      }\n\n      &.caption {\n        color: #FFF;\n        font-weight: 300;\n        font-size: 20px;\n        margin-top: 0;\n      }\n    }\n\n    p {\n      font-family: \"Lato\", sans-serif;\n      font-weight: 400 !important;\n      font-size: 20px;\n      margin: 30px 0 35px;\n\n      span {\n        color: #FFF;\n        padding: 5px 15px;\n        background-color: rgba($teal, $span-opacity);\n      }\n    }\n\n    // set the colour with primary/secondary -- the .donate.button class will be used for the modal\n    //.donate.button {\n    //  background: $orange !important;\n    //}\n  }\n}\n","body {\n  margin: 0;\n  padding: 0;\n  font-family: \"Lato\", sans-serif;\n  font-size: 16px;\n}\n\n@import \"mixins/responsive\";\n@import \"variables/breakpoints\";\n@import \"variables/colors\";\n@import \"layout/header\";\n@import \"layout/footer\";\n@import \"layout/navigation\";\n@import \"helpers/helpers\";\n@import \"elements/accordion\";\n@import \"elements/inputs\";\n@import \"pages/homepage\";\n@import \"pages/careers\";\n@import \"pages/scholars\";\n@import \"pages/connect\";\n@import \"pages/get-involved\";\n@import \"pages/success-stories\";\n@import \"pages/who-we-support\";\n@import \"pages/about\";\n@import \"posts/post\";\n","#navigation-bar {\n  $animation-duration: 0.25s;\n  padding: 15px;\n\n  .logo {\n    display: inline-block;\n\n    img {\n      height: 55px;\n    }\n  }\n\n  &.scrolling {\n    background: rgba(255, 255, 255, 0.98);\n    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.15);\n    margin-top: 0;\n    top: -70px;\n    opacity: 0;\n    position: fixed;\n\n    &.slide-down {\n      top: 0;\n      opacity: 1;\n      transition: all $animation-duration ease;\n    }\n\n    &.slide-up {\n      top: -70px;\n      opacity: 0;\n      transition: all $animation-duration ease;\n    }\n  }\n\n  &.mobile.open {\n    box-shadow: 0 0 0;\n    height: 100vh;\n    overflow: hidden;\n    width: 100vw;\n    position: fixed;\n    top: 0;\n    bottom: 0;\n    left: 0;\n    right: 0;\n    background: #fff;\n    z-index: 100;\n    transition: position $animation-duration ease;\n\n    .mobile.menu.toggle {\n      a {\n        span {\n          background-color: transparent;\n\n          &::before {\n            top: 0;\n            transform: rotate(45deg);\n          }\n\n          &::after {\n            top: 0;\n            bottom: auto;\n            transform: rotate(-45deg);\n          }\n        }\n      }\n    }\n  }\n\n  &.scrolling,\n  &.mobile.open {\n    .menu.mobile.toggle {\n      span {\n        &,\n        &::before,\n        &::after {\n          background-color: $black;\n        }\n      }\n    }\n\n    .menu ul {\n      > li {\n        > a {\n          color: $black;\n        }\n      }\n    }\n  }\n\n  > .ui.container {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    transition: width 0.35s ease, padding 0.35s ease;\n\n    @media screen and (min-width: $tablet-breakpoint) and (max-width: 1500px) {\n      width: 100%;\n      padding: 0 20px;\n    }\n  }\n\n  .menu {\n    opacity: 1;\n    transition: opacity $animation-duration ease;\n    width: 100%;\n\n    &.mobile.toggle {\n      display: none;\n\n      a {\n        display: block;\n        position: relative;\n        font-size: 24px;\n        width: 1em;\n        height: 1em;\n        line-height: 1;\n\n        span {\n          position: absolute;\n          top: 0.45em;\n          left: 0;\n          right: 0;\n          height: 0.1em;\n          background: black;\n          text-indent: -9999999px;\n          transition: all $animation-duration ease;\n\n          &::before,\n          &::after {\n            content: '';\n            position: absolute;\n            left: 0;\n            right: 0;\n            height: 0.1em;\n            background: black;\n            transition: all $animation-duration ease;\n          }\n\n          &::before {\n            top: -0.3em;\n          }\n\n          &::after {\n            bottom: -0.3em;\n          }\n        }\n      }\n    }\n\n    @media (max-width: 995px) {\n      &.desktop.links {\n        display: none;\n      }\n\n      &.mobile.toggle {\n        display: block;\n        width: auto;\n      }\n    }\n\n    > ul {\n      list-style: none;\n      margin: 0;\n      padding: 0;\n      display: flex;\n      flex-direction: row;\n      align-items: center;\n\n      > li {\n        margin: 0;\n        padding: 0;\n        position: relative;\n        height: 100%;\n        flex-direction: column;\n        flex: 1 0 auto;\n\n        @include tablet {\n          padding: 0 10px;\n        }\n\n        &.selected {\n          > a {\n            background: $blue;\n            color: white;\n          }\n        }\n\n        > a {\n          display: block;\n          color: black;\n          font-size: 16px;\n          text-transform: uppercase;\n          transition: all $animation-duration ease;\n          will-change: color;\n          padding: 6px 8px;\n          text-align: center;\n\n          &.fixed {\n            background: $accent;\n            color: #FFF;\n            margin-left: 10px;\n          }\n\n          @include computer {\n            &:hover {\n              background: $blue;\n              color: white;\n            }\n          }\n        }\n\n        position: relative;\n\n        &:hover {\n          ul {\n            opacity: 1;\n            visibility: visible;\n          }\n        }\n\n        ul {\n          position: absolute;\n          opacity: 0;\n          visibility: hidden;\n          left: 0;\n          display: inline-block;\n          margin: 0;\n          padding: 0;\n          top: 100%;\n          min-width: 195px;\n          transition: 0.3s ease all;\n          z-index: 999;\n          list-style: none;\n          margin: 0;\n          padding: 0;\n\n          a {\n            font-family: \"Lato\", sans-serif;\n            font-weight: 400 !important;\n            text-transform: uppercase;\n            background: #FFF;\n            opacity: 1;\n            transition: 0.3s ease all !important;\n            border-radius: 0 !important;\n            display: block;\n            padding: 10px 15px;\n            color: $black;\n\n            &:hover {\n              background: $accent;\n              color: #FFF;\n            }\n\n            &.search {\n              &:hover {\n                background: none;\n              }\n            }\n\n            &.fixed {\n              background: $accent;\n              color: #FFF;\n              margin-left: 10px;\n            }\n          }\n        }\n      }\n    }\n  }\n\n  .logo {\n    padding: 15px 0;\n    padding-right: 10px;\n    transition: padding $animation-duration ease;\n\n    a {\n      display: block;\n\n      svg {\n        width: 211px;\n        height: 37px;\n        vertical-align: top;\n\n        @include mobile {\n          width: 171px;\n          height: 30px;\n        }\n\n        .logo-text {\n          transition: fill $animation-duration ease;\n        }\n      }\n\n      &:hover {\n        svg {\n          .logo-text {\n            fill: $light-blue !important;\n          }\n        }\n      }\n    }\n  }\n\n  .ui.mobile.container {\n    position: relative;\n    z-index: 110;\n    display: none;\n    text-align: center;\n    font-weight: 700;\n    padding-bottom: 2rem;\n\n    nav {\n      > ul {\n        border-top: 1px solid $grey;\n        border-bottom: 1px solid $grey;\n        padding: 0.75em 0;\n        margin: 0 0 2rem;\n        list-style: none;\n        font-size: 16px;\n        font-weight: 700;\n        text-transform: uppercase;\n        user-select: none;\n\n        > li {\n\n          a {\n            position: relative;\n            display: inline-block;\n            padding: 0.75em 0;\n            color: $black;\n          }\n\n          ul {\n            display: none;\n            list-style: none;\n            padding: 0;\n            margin: 0;\n\n            li {\n              a {\n                color: $dark-grey;\n                font-size: 16px;\n                padding: 0.5em 0;\n                text-transform: none;\n              }\n            }\n          }\n        }\n      }\n    }\n\n    .ui.button {\n      padding: 0.8rem 2rem;\n    }\n\n    a.call {\n      color: $black;\n      font-size: 1.1rem;\n\n      .icon {\n        color: $light-blue;\n      }\n    }\n  }\n}\n\n#primary-menu {\n  padding-left: 30px;\n  max-width: 900px;\n}\n\n.mobile.slice {\n  position: fixed;\n  left: 0;\n  display: block;\n  width: 100%;\n  height: 100vh;\n  background: #fff;\n  transform: matrix(1, -0.3, 0, 1, 0, 0);\n  transition: top 0.35s ease-out, bottom 0.5s ease-out;\n  z-index: 95;\n  border-top: 1px solid $grey;\n  border-bottom: 1px solid $grey;\n\n  @include not-mobile {\n    display: none;\n  }\n\n  &.active {\n    transition: top 0.35s ease, bottom 0.5s ease, border-color 0.2s ease 0.45s;\n    border-color: white;\n  }\n}\n","@mixin mobile {\n  @media (max-width: #{$tablet-breakpoint - 1px}) {\n    @content;\n  }\n}\n\n@mixin not-mobile {\n  @media (min-width: #{$tablet-breakpoint}) {\n    @content;\n  }\n}\n\n@mixin tablet {\n  @media (min-width: #{$tablet-breakpoint}) and (max-width: #{$computer-breakpoint - 1px}) {\n    @content;\n  }\n}\n\n@mixin not-computer {\n  @media (max-width: #{$computer-breakpoint - 1px}) {\n    @content;\n  }\n}\n\n@mixin computer {\n  @media (min-width: #{$computer-breakpoint}) {\n    @content;\n  }\n}\n\n@mixin small-monitor {\n  @media (min-width: #{$computer-breakpoint}) and (max-width: #{$large-monitor-breakpoint - 1px}) {\n    @content;\n  }\n}\n","#footer {\n  background-color: #f7f7f7;\n  padding-top: 5px;\n\n  span.bigger {\n    font-size: 14px;\n    margin-bottom: 15px;\n    display: block;\n  }\n\n  .social {\n    a {\n      font-size: 20px;\n      display: inline-block;\n      margin: 5px 0;\n    }\n\n    .scholars {\n      a {\n        color: $orange;\n      }\n    }\n\n    .careers {\n      a {\n        color: $teal;\n      }\n    }\n\n    .connect {\n      a {\n        color: $red;\n      }\n    }\n  }\n\n  a {\n    color: $blue;\n  }\n\n  .ui.grid {\n    margin-top: 0;\n  }\n\n  .top {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    flex-wrap: wrap;\n    padding-top: 0.5rem;\n    margin-bottom: 0.5rem;\n\n    @include not-computer {\n      justify-content: center;\n    }\n\n    ul {\n      list-style: none;\n      margin: 0;\n      display: inline-flex;\n      align-items: center;\n      padding: 30px 0 0 0;\n\n      @include not-computer {\n        display: flex;\n        flex-wrap: wrap;\n        padding-top: 20px;\n        flex-basis: 100%;\n        justify-content: center;\n      }\n\n      li {\n        margin: 0;\n        display: inline-block;\n        font-size: 14px;\n      }\n    }\n\n    ul.breadcrumbs {\n      padding-top: 0;\n\n      @include mobile {\n        display: none;\n      }\n\n      @include tablet {\n        margin-bottom: 1rem;\n      }\n\n      li {\n        position: relative;\n        padding: 0 30px 0 0;\n        height: 36px;\n        line-height: 36px;\n\n        &:after {\n          display: block;\n          content: \"\\f105\";\n          position: absolute;\n          top: 0;\n          right: 12px;\n          font-family: Icons;\n        }\n\n        &:last-child {\n          padding-right: 0;\n\n          &:after {\n            display: none;\n          }\n        }\n\n        &.logo {\n          display: block;\n          height: auto;\n\n          a {\n            display: block;\n          }\n\n          img {\n            width: 50px;\n            height: auto;\n            display: block;\n          }\n        }\n      }\n\n      img, object {\n        width: 30px;\n        height: 30px;\n      }\n    }\n\n    ul.top-links {\n      padding-top: 0;\n\n      @include mobile {\n        justify-content: center;\n      }\n\n      li {\n        color: $very-dark-blue;\n        font-weight: 600;\n        padding: 0 0 0 30px;\n        text-align: center;\n\n        @include mobile {\n          padding: 10px;\n\n          &:first-child {\n            flex-basis: 100%;\n          }\n        }\n\n        &:first-child {\n          padding-left: 0;\n        }\n\n        a {\n          color: $very-dark-blue;\n\n          &:hover {\n            color: $blue;\n          }\n\n          .icon {\n            color: $blue;\n          }\n        }\n      }\n    }\n  }\n\n  ul {\n    list-style: none;\n    margin: 0;\n    padding: 0;\n\n    li {\n      display: block;\n      margin: 2px 0;\n      padding: 0;\n    }\n  }\n\n  .middle.row {\n    padding-top: 3rem;\n    padding-bottom: 3rem;\n\n    .social.links.dropdown.column {\n      a {\n        font-size: 1.4rem;\n        margin-right: 1rem;\n      }\n    }\n\n    @include mobile {\n      padding-top: 0;\n      padding-left: 1rem;\n      padding-right: 1rem;\n\n      > .column {\n        padding-left: 0 !important;\n        padding-right: 0 !important;\n      }\n\n      > .dropdown.column {\n        padding-top: 0 !important;\n        padding-bottom: 0 !important;\n        border-bottom: 1px solid $grey;\n\n        .title {\n          position: relative;\n          padding-top: 1rem;\n          padding-right: 2rem;\n          padding-bottom: 1rem;\n          margin: 0;\n\n          &:first-child {\n              cursor: pointer;\n          }\n\n    