    color: white;\n      }\n    }\n\n    &.prev {\n      //left: 40px;\n\n      &:before {\n        content: \"\\f137\";\n        font-family: Icons;\n        font-size: 40px;\n        color: white;\n      }\n    }\n  }\n\n  .unslider-nav {\n    margin-bottom: 30px;\n\n    li {\n      width: 12px;\n      height: 12px;\n      border-radius: 999px;\n    }\n  }\n}\n\n.masthead {\n  background-size: cover;\n  background-repeat: no-repeat;\n  background-position: center center;\n  width: 100%;\n  position: relative;\n\n  .button {\n    margin-top: 25px;\n  }\n\n  &:not(.about-page) {\n\n    &:before {\n      content: '';\n      display: block;\n      position: absolute;\n      top: 0;\n      right: 0;\n      bottom: 0;\n      left: 0;\n      background: rgba(0, 0, 0, 0.2);\n    }\n\n    >.grid {\n      height: 75vh;\n    }\n\n  }\n\n  $span-opacity: 0.6;\n\n  .grid h1.blue span {\n    background-color: rgba($blue, $span-opacity);\n  }\n\n  &.connect {\n    .grid h1 span {\n      background-color: rgba($red, $span-opacity);\n    }\n  }\n\n  &.scholars {\n    .grid h1 span {\n      background-color: rgba($orange, $span-opacity);\n    }\n  }\n\n  &.careers-page {\n    .grid h1 span {\n      background-color: rgba($teal, $span-opacity);\n    }\n    .buttons {\n      text-align: center !important;\n      .button {\n        display: block;\n        @include mobile {\n          margin: 10px 0;\n        }\n      }\n    }\n  }\n\n  .buttons {\n    .button {\n      display: inline-block;\n      max-width: 175px;\n      margin-left: auto;\n      margin-bottom: 5px;\n\n      @include not-computer {\n        margin: 0;\n      }\n    }\n  }\n\n  .grid {\n\n    h1 {\n      font-family: \"Lato\", sans-serif;\n      font-weight: 300 !important;\n      font-size: 2rem;\n      text-transform: uppercase;\n      line-height: 3rem;\n\n      span {\n        color: #FFF;\n        padding: 5px 15px;\n        background-color: rgba($teal, $span-opacity);\n\n        @include not-computer {\n          display: block;\n        }\n      }\n    }\n\n    h2 {\n      span {\n        color: #FFF;\n        padding: 5px 15px;\n        background-color: rgba($teal, $span-opacity);\n      }\n\n      &.caption {\n        color: #FFF;\n        font-weight: 300;\n        font-size: 20px;\n        margin-top: 0;\n      }\n    }\n\n    p {\n      font-family: \"Lato\", sans-serif;\n      font-weight: 400 !important;\n      font-size: 20px;\n      margin: 30px 0 35px;\n\n      span {\n        color: #FFF;\n        padding: 5px 15px;\n        background-color: rgba($teal, $span-opacity);\n      }\n    }\n\n    // set the colour with primary/secondary -- the .donate.button class will be used for the modal\n    //.donate.button {\n    //  background: $orange !important;\n    //}\n  }\n}\n","body {\n  margin: 0;\n  padding: 0;\n  font-family: \"Lato\", sans-serif;\n  font-size: 16px;\n}\n\n@import \"mixins/responsive\";\n@import \"variables/breakpoints\";\n@import \"variables/colors\";\n@import \"layout/header\";\n@import \"layout/footer\";\n@import \"layout/navigation\";\n@import \"helpers/helpers\";\n@import \"elements/accordion\";\n@import \"elements/inputs\";\n@import \"pages/homepage\";\n@import \"pages/careers\";\n@import \"pages/scholars\";\n@import \"pages/connect\";\n@import \"pages/get-involved\";\n@import \"pages/success-stories\";\n@import \"pages/who-we-support\";\n@import \"pages/about\";\n","#navigation-bar {\n  $animation-duration: 0.25s;\n  padding: 15px;\n\n  .logo {\n    display: inline-block;\n\n    img {\n      height: 55px;\n    }\n  }\n\n  &.scrolling {\n    background: rgba(255, 255, 255, 0.98);\n    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.15);\n    margin-top: 0;\n    top: -70px;\n    opacity: 0;\n    position: fixed;\n\n    &.slide-down {\n      top: 0;\n      opacity: 1;\n      transition: all $animation-duration ease;\n    }\n\n    &.slide-up {\n      top: -70px;\n      opacity: 0;\n      transition: all $animation-duration ease;\n    }\n  }\n\n  &.mobi